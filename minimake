	echo "#define mini_globals_on_stack"
}

mini_globals(){
	echo "#define mini_globals"
}

define(){
	param=$1
	shift
	echo "#define" $param \"$*\"
}

DEFINE(){
	param=$1
	shift
	echo "#define" $param \"$*\"
	#echo "#define" $1 $2
}

embed(){
	export embed="$*"
}

mini_getopt(){
	echo "#define mini_getopt"
}

mini_getopt_long(){
	echo "#define mini_getopt_long"
}


mini_define(){
	if [ ! -z $2 ]; then 
		echo "#define mini_$1 $2" 
	else 
		echo  "#define mini_$1 $1"
	fi
}


