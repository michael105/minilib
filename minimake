LMAGENTA=""$'\033[01;35m'""
CYAN=""$'\033[36m'""
LCYAN=""$'\033[01;36m'""
WHITE=""$'\033[01;37m'""
LGRAY=""$'\033[37m'""


touchdummyheaders(){
	if [ ! -d $BUILDDIR ]; then
		mkdir $BUILDDIR
	fi
	if [ ! -d "$BUILDDIR/dummyheader" ]; then
		mkdir $BUILDDIR/dummyheader
	fi

	tmp=`pwd`
	cd $BUILDDIR/dummyheader && \
	mkdir bits sys net arpa scsi netinet protocols && \
	for i in bits/uClibc_config.h bits/posix_opt.h bits/local_lim.h bits/fcntl.h bits/byteswap.h bits/shm.h bits/epoll.h bits/sem.h bits/mman.h bits/sigcontext.h bits/msq.h bits/environments.h bits/fenv.h bits/wordsize.h bits/endian.h bits/mathdef.h bits/stat.h bits/mathinline.h bits/setjmp.h bits/byteswap-16.h bits/wchar.h bits/uClibc_touplow.h bits/uClibc_charclass.h bits/getopt_int.h bits/socket_type.h bits/utmp.h bits/errno.h bits/statx.h bits/huge_vall.h bits/termios.h bits/ioctls.h bits/sigaction.h bits/typesizes.h bits/sigthread.h bits/fenvinline.h bits/uClibc_local_lim.h bits/uClibc_locale.h bits/xopen_lim.h bits/confname.h bits/uClibc_page.h bits/timerfd.h bits/dirent.h bits/byteswap-common.h bits/huge_valf.h bits/stdio.h bits/waitstatus.h bits/utsname.h bits/eventfd.h bits/ipc.h bits/posix1_lim.h bits/uClibc_clk_tck.h bits/mman-shared.h bits/siginfo.h bits/netdb.h bits/stdio_lim.h bits/nan.h bits/waitflags.h bits/mqueue.h bits/statvfs.h bits/in.h bits/uClibc_alloc.h bits/resource.h bits/mathcalls.h bits/uio.h bits/sigset.h bits/kernel-features.h bits/poll.h bits/mman-common.h bits/signum.h bits/huge_val.h bits/locale.h bits/utmpx.h bits/signalfd.h bits/sigstack.h bits/select.h bits/statfs.h bits/ioctl-types.h bits/inf.h bits/uClibc_posix_opt.h bits/dlfcn.h bits/uClibc_stdio.h bits/sockaddr.h bits/sched.h bits/socket.h bits/elfclass.h bits/initspin.h bits/posix2_lim.h bits/types.h bits/time.h bits/getopt.h bits/inotify.h bits/cmathcalls.h bits/mman-linux.h bits/sysnum.h bits/semaphore.h bits/pthreadtypes.h sys/perm.h sys/ucontext.h sys/io.h sys/reg.h sys/debugreg.h sys/procfs.h sys/user.h sys/timerfd.h sys/ptrace.h sys/prctl.h sys/fanotify.h sys/epoll.h sys/acct.h sys/random.h sys/eventfd.h sys/signalfd.h sys/inotify.h sys/vfs.h sys/swap.h sys/timeb.h sys/types.h sys/timex.h sys/syscall.h sys/socket.h sys/select.h sys/ipc.h sys/kdaemon.h sys/kd.h sys/unistd.h sys/dir.h sys/sysmacros.h sys/reboot.h sys/msg.h sys/mman.h sys/statvfs.h sys/bitypes.h sys/time.h sys/xattr.h sys/syslog.h sys/file.h sys/socketvar.h sys/times.h sys/poll.h sys/mount.h sys/stat.h sys/shm.h sys/un.h sys/statfs.h sys/personality.h sys/resource.h sys/fcntl.h sys/wait.h sys/utsname.h sys/fsuid.h sys/signal.h sys/errno.h sys/quota.h sys/param.h sys/sem.h sys/soundcard.h sys/cdefs.h sys/sendfile.h sys/ioctl.h sys/ttydefaults.h sys/sysinfo.h sys/queue.h sys/termios.h sys/vt.h sys/mtio.h sys/klog.h sys/uio.h sys/sysctl.h net/if_arp.h net/if.h net/if_slip.h net/ppp_defs.h net/route.h net/if_shaper.h net/ethernet.h net/ppp-comp.h net/if_packet.h net/if_ppp.h arpa/ftp.h arpa/nameser.h arpa/tftp.h arpa/inet.h arpa/nameser_compat.h arpa/telnet.h scsi/scsi_ioctl.h scsi/sg.h scsi/scsi.h netinet/tcp.h netinet/ether.h netinet/in_systm.h netinet/igmp.h netinet/if_ether.h netinet/in.h netinet/udp.h netinet/ip6.h netinet/if_tr.h netinet/ip.h netinet/icmp6.h netinet/if_fddi.h netinet/ip_icmp.h fpu_control.h thread_db.h pthread.h semaphore.h libgen.h ucontext.h stdint.h grp.h limits.h fts.h termio.h syscall.h mqueue.h utmpx.h inttypes.h pwd.h regex.h langinfo.h unistd.h a.out.h tar.h wchar.h strings.h printf.h search.h time.h fnmatch.h sysexits.h glob.h elf.h utmp.h stdlib.h math.h wordexp.h link.h syslog.h utime.h ttyent.h poll.h malloc.h byteswap.h nl_types.h features.h complex.h error.h uchar.h stdio_ext.h lastlog.h fcntl.h paths.h wait.h values.h string.h sched.h ifaddrs.h dlfcn.h endian.h signal.h errno.h gnu-versions.h memory.h mntent.h resolv.h crypt.h cpio.h ar.h setjmp.h regexp.h netdb.h locale.h shadow.h stdio.h tgmath.h pty.h ulimit.h ieee754.h getopt.h spawn.h termios.h sgtty.h err.h ftw.h fenv.h wctype.h dirent.h alloca.h execinfo.h ctype.h assert.h protocols/routed.h protocols/talkd.h protocols/timed.h protocols/rwhod.h
	do 
		touch $i
	done
	cd $tmp
}



# ==================================================
# configfiles and embedded scripts start below. 
# They are generated and included by "make minimake"
# All changes below will be overwritten.
# ==================================================

# prefer the genconfig script, if present.
if [ -e $MLIBDIR/scripts/genconfig.sh ]; then
	#echo_verbose Parsing genconfig.sh
	source $MLIBDIR/scripts/genconfig.sh
else
	verbose "Parsing builtin config"	

# --- genconfig-start: TMPL ---
#
# Script, updated from "make header" / genheaders.pl
# The functions are callen from minilib.conf 
# (or another config file)
# The output of this script generates minilib.conf.h
# 

# empty, to enable parsing 'makefiles'
ifdef(){ 
	echo -n
}

globalregister(){
	if [ ! -z $1 ]; then 
		echo "#define mini_globalregister $1" 
	else 
		echo  '#define mini_globalregister "r15"'
	fi
}


mini_errno(){
	echo "#define mini_errno $1"
}

compile(){
	for f in `echo $* | tr ',' ' '`;
	do "mini_$f"
	done
}

COMPILE(){
	for f in `echo $* | tr ',' ' '`;
	do "mini_$f"  
	done
}

VERBOSE(){
	export verbose="1"
}

mini_start(){
	echo "#define mini_start $1"
}
mini_exit(){
	echo "#define mini_exit $1"
}

mini_setjmp(){
	echo "#define mini_setjmp"
}

