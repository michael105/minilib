#!/bin/bash

VERSION="20211019"


# colorize the output
ansicolor=1

# =1 :use the minilib, included within this script as zipped file.
# =0 :use the individual files, when existing within MLIBDIR
useincludedminilib=1

# use clang
# CLANG=1

defaultvalues (){
#everything between _start and _end is extracted and copied into Makefile.minilib
# ------------------
#defaultvalues_start

# gcc/clang options
# only linux x64 for now.

if [ -n "$CC" ]; then
	REAL_CC="$CC"
	echo $CC | sed '/clang/Q1' > /dev/null
	if [ $? = 1 ]; then
		CLANG=1
	fi
else
	if [ "$CLANG" = 1 ]; then
		REAL_CC="clang"
	else
		REAL_CC="gcc"
	fi
fi

#echo $REAL_CC | sed '/gcc/Q1' > /dev/null
#if [ $? = 1 ]; then
#	echo $REAL_CC | sed '/9.3/Q1' > /dev/null
#	if [ $? != 1 ]; then
#		echo 
#	fi
#fi


if [ -z "$BUILDDIR" ]; then
	BUILDDIR="build"
fi

if [ -z "$OPTFLAG" ]; then
	OPTFLAG="-Os"
fi

if [ -z "$PICFLAG" ]; then
	PICFLAG="-fno-pic"
fi

#strip symbols
if [ -z "$STRIPFLAG" ]; then
	STRIPFLAG="-s"
fi

# No OS detections. Currently only Linux 64bit 
if [ -z "$ARCH" ]; then
	MINI_ARCH="X64"
fi

if [ -z "$OS" ]; then
	MINI_OS="LINUX"
fi

COMMONCFLAGS="-I. -fno-stack-protector -fdata-sections -fomit-frame-pointer -ffunction-sections \
	-Wno-unused-variable -Wno-unused-const-variable -Wall -Wno-unused\
	-nodefaultlibs -static -nostdlib -DMLIB -fno-builtin "
	#-nodefaultlibs -static -nostdlib -DMLIB -fno-builtin -mmanual-endbr "


CFLAGS="$CFLAGS $COMMONCFLAGS -DLINUX -DX64 "

# somehow; seems to be neccessary telling clang exclusively to use it's own linker.?
if [ "$CLANG" = 1 ]; then
	CFLAGS="$CFLAGS -fuse-ld=lld "
else
	CFLAGS="$CFLAGS -Wno-unused-but-set-variable "
fi


# ld options 
LDFLAGS="-Wl,-n,--gc-sections,-nostdlib,--build-id=none,--allow-multiple-definition,-z,noexecstack"

# (default) linker script. minimake is going to be invoked with minimake --dump ld_script 
if [ -z "$LDSCRIPT" ]; then
	LDSCRIPT="default"
fi

# colorize the output of gcc
colorize='2>&1| sed -e "s/^\S.* warning:.*/$YELLOW\0$NORM/" -e "s/note:.*/$MAGENTA\0$NORM/" -e "s/\s\^\~*/$MAGENTA\0$NORM/" -e "s/\S*\.[c,h,cpp,hpp] /$CYAN\0$NORM/" -e "s/^.*:\serror:.*/$RED\0$NORM/" -e "s/\(^\S*\/\)\(\S*\.[h|c]:\s\)/\1$CYAN\2$NORM/" -e "/error:/h;\${p;g;/\S/Q1}" -e "s/\d226/\"/g"'

#defaultvalues_end
# --- End of makefile inclusion
}


# grep minimake path 
MLIBDIR=$(echo -e $0 | sed -E "s/(.*)\/minimake/\1/")
if [ ! -e "$MLIBDIR/minilib.h" ]; then
	MLIBDIR="."
	if [ ! -e "$MLIBDIR/minilib.h" ]; then
		MLIBDIR="/home/micha/prog/minilib"  # .. I know ;)
	fi
fi

# Needed when using the minilib.h of the devel branch.
INCLUDEDIRS="-I$MLIBDIR/headers/common-links -I$MLIBDIR/headers/common \
	-I$MLIBDIR/headers/linux_x64 -I$MLIBDIR -I./ -I$MLIBDIR/include -I$MLIBDIR"


if [ ! -e "$MLIBDIR/minilib.h" ]; then # minilib not found. check, if included
	if [  "`sed -n '/^#MINILIBGZ/,+3p' $0 | sed -n '3s/.*/1/p'`" = 1 ];then
		useincludedminilib=1
	fi
fi


echo_verbose(){
	if [ "$VERBOSE" == "1" ]; then
		>&2 echo $@
	fi
}


verbose(){
	if [ "$VERBOSE" == "1" ]; then
		>&2 echo $@
	fi
}

fail(){
	echo $RED$* $NORM
	exit 1
}


PARSE=1

while [ 1 ]; do # loop around the whole script, 
# source functions and variables
# Nicer, to have all this data at the bottom than on top
if [ $PARSE = 0 ]; then 

usage(){
	echo "minimake v.$VERSION"
	echo
	echo "Usage: \`minimake [options] [gcc-options] [-o outputfile] inputfile(s).c|inputfile.conf\`

minimake compiles binaries and links them statically with minilib.
It's a wrapper to call gcc or clang with the right compile options,
generate the config of minilib and include minilib.h. 

In the most shorthand form, the only argument is the file,
containing the configuration directives. (And possibly the c source)

minimake passes all options it's invoked with on to the compiler,
besides the options described below, which must be passed 
before options and parameters for gcc.
Beginning with the first option, not recognized as option for minimake,
all options are given unchanged to gcc/clang.

-h, --help:    
        shows this help

--version:     
        version info

--license:     
        shows the license (Opensource, fair use by attribution)

-v,--verbose: 
        work verbosely

--config 'file': 
        when the --config 'file' option is given
        minimake parses the config in 'file',
        and includes the generated define switches
        into every compiled file via the gcc option -include and pipes. 
        The configfile syntax is described within the file
        'minilib.mconf', which is also a minimal configuration file
        for the hello world examples.
        When the file 'minilib.h' is not found, 
        minilib.h will be piped to gcc on the fly.

--dump-config 'file':
        Parses 'file' as configuration file, and dumps the generated
        define switches to stdout.
        Used internally, to pass the generated header via pipe to gcc

--dump-sources 'file1' 'file2' .. / 'configfile':
        Parses 'configfile' as configuration file, and dumps the sources
        to stdout.
        Used internally, to pass the generated header via pipe to gcc

--showsources
        Dump all used sources to the terminal 
        (gcc options -E -P)

--no-mbuf:
        When generating a config, don't write switches for the global mini_buf, 
        also don't generate the global structures for the fstream streams. 
        (Handle them as simple numeric file descriptors within the according
        minilib fstream functions)
        Without global data some initialization code can be saved,
        as well as all elf segments besides 'text'.
        Either add the line into your configfile: 'LDSCRIPT onlytext'
        Or use the switch '--ldscript onlytext'

--ldscript-file 'file':
        Uses the supplied 'file' as linker script for ld.

--dump 'var':
        Dumps the content of 'var' to stdout

--ldscript 'name': 
        Either includes the according ld.script.'name' file 
        from the directory ldscripts, if existent, or extracts the according
        script and writes it into minilib.ldscript.
        This script is passed to ld with the option -T
        name can be one of:
          - onlytext     ( drop all sections beside the text segment )
          - text_and_bss ( keep also the bss segment )

--autoconf / -ac
        This is going to generate the configuration file 'minilib.mconf'
        and compile the supplied sources with this configuration
        It is essentially the same, as calling 'minimake --genconf minilib.mconf',
        and 'minimake --config minilib.mconf [args]

[--force] --genconf 'outfile' 'infile1' 'infile2' ...:
        parses the sourcefiles 'infile1',.. and writes a config into
        outfile, which enables compiling of all used minilib functions
        within 'infiles'. 
        Most possibly you'll still have to edit the config manually,
        e.g. printf conversions aren't correctly recognized.
        After the config is written to outfile, 
        use the config file with --config 'outfile' 
          --force
            Overwrite existing files without warning

--shrinkelf:
        Shrink the resulting elf binary via tools/shrinkelf.
        TODO: implement proper search algorithm. 
        Prob. keep shrinkelf's sourcecode within minimake (?)

--dump var
        dump the content of 'var', if defined.

--dump-minilib
        dump the contained headerfile minilib.h to stdout
        (if bundled within minimake)

--cc
        The following arguments are passed unchanged to gcc.
        It's not strictly necessary, to pass this parameter;
        the first argument not recognized will do the same
        and be passed on to gcc

--exec 'sourcefile'
        Compile sourcefile to 'basename sourcefile .c';
        and execute the binary 
        (works only with embedded 'define' configuration yet) (TODO)

-mlc, --compile 'sourcefile'
        compile sourcefile, use embedded scripting config
        (same as: --config 'sourcefile' -o \`basename sourcefile\` 'sourcefile')

--dlog  debug log, write debug info to 'debuglog.n' (n counting up)

--compat
        Include the provided compat headers (stdio.h,stdlib.h,...) directory 
        into the include search path.
        Using them is not recommended, they are not stable.

--fulldebug generate an outputfile (sourcefile.dbg.c) including the used parts
        of minilib, and compile this file to sourcefile.dbg with the options
        -g2 -Og set.


---------


The following (optional) environmental variables are used:

CC          The name and (optional) path of the compiler
CFLAGS      optional flags for the compiler
CLANG       Set this to 1 to use clang as compiler.
            (Or set CC to clang)
OPTFLAG     optimization flag (default -Os)
STRIPFLAG   stripflag (default -s)
PICFLAG     parameter -fpic/-fno-pic .. (default -fno-pic)
LDFLAGS     (optional) flags for the linker
LDSCRIPT    Name of the ldscript
VERBOSE     Set this to 1 to enable verbose messages
NOCOLOR     Set this to 1 to disable colors
SHOWSOURCES Set to 1 to save all combined sources into the outputfile


(c) 2019-2021 Michael misc Myer. All rights reserved.
 ('minimake --license' shows the license)

"
		exit
}

ARGS=$@

if [ "$1" = "" ]
then
		usage
fi

opt=1
opt_force=0
opt_autoconf=0
opt_exec=0
opt_execname=""
outputfile="a.out"
dump_cmd=0

# Handle only one argument.
# use either the embedded config ( first line #if 0 )
# or one of ( in this order ): file.conf, file.mconf, minilib.conf,
# minilib.mconf
if [ "$2" == "" ]; then
	if [ "$1" == "-h" ] || [ "$1" == "--help" ]; then
			usage
	fi
	if [ "$1" == "--license" ]; then
			echo "$LICENSE"
			exit
	fi
	if [ "$1" == "--version" ]; then
						echo $VERSION
						exit 0
	fi

	if [ `echo $1 | dd bs=1 count=1 2>/dev/null` != "-" ]; then	
	config=$1
	if [ ! -f $config ]; then
		fail "No such file: $config"
	fi
	sed -n -e '1,2{/#mlconfig/q1}' -e '1,2{/#if 0/q1}' $config
	if [ $? != 1 ]; then
		echo ${YELLOW}No configuration found in $config $NORM
		tmp=`echo $1 | sed -E 's/(.*)\..*/\1/'`
		for config in $tmp.conf $tmp.mconf minilib.conf minilib.mconf
		do
			echo Try: $config
			if [ -f $config ]; then
				echo Using config: $config
				break
			fi
		done
		if [ ! -f $config ]; then
			fail "No configuration found"
		fi
	fi

	verbose $REAL_CC config: $config
	outputfile=`echo $1 | sed -E 's/(.*)\..*/\1/'`
	checkconfig $config
	#verbose sources: $sources
	verbose compile: $outputfile
	opt_config=$config
	if [ "$1" == "$outputfile" ]; then
		fail "couldn't determine the outputfile.\nGot: $1"
	fi
	if [ -z "$sources" ]; then
		export sources=$config 
	fi
	#REAL_CC="$REAL_CC -o $outputfile $1"
	#echo $REAL_CC
	echo $GREEN"make $config"$NORM 
	compile=1
	opt=0
	shift
	fi
fi

# get the ouput file's name
a=0
for i in "$@"
do
	if [ $a = 1 ]; then
		outputfile=$i
		a=0
		break
	fi
	if [ $i == "-o" ]
	then
		a=1
	fi
done

#echo_verbose "output: $outputfile"
while [ "$1" != "" ] && [ $opt = 1 ]
do
	opt=0
	case $1 in
		--cc )
			shift
			opt=0
			;;
		--gcc )
			shift
			opt=0
			;;
		--dlog )
			shift
			opt=1
			debuglog=1
			;;
		--version )
			echo $VERSION
			exit 0
			;;
		--verbose | -v )
			VERBOSE=1
			shift
			opt=1
			;;
		--dump-cmd )
			shift
			opt=1
			dump_cmd=1
			;;
		--showsources )
			shift
			opt=1
			SHOWSOURCES=1
			;;
		--useincludedminilib )
			shift
			opt=1
			useincludedminilib=1
			;;

		--dump-minilib )
			if [ "$useincludedminilib" = "0" ] && [ -e $MLIBDIR/minilib.h ]
			then 
				cat $MLIBDIR/minilib.h
			else
				sed -e '0,/^#MINILIBGZ#$/d;/^#ENDGZ/Q' $0 | head -q -c -1 | gunzip -c
			fi
			exit 0
			;;
		--dump )
			if [ "${!2}" == "" ] ; then
				echo No such var: $2 >&2
				exit 1
			else
				echo "${!2}"
				exit 0
			fi
			;;
		--config )   
			shift
			config=$1
			checkconfig $config 
			opt_config=$1
			compile=1
			shift
			opt=1
			;;
		-mlc | --compile )
			shift
			config=$1
			echo $REAL_CC config: $config
			checkconfig $config
			opt_config=$1
			outputfile=`basename $1 .c`
			REAL_CC="$REAL_CC -o $outputfile $1"
			echo $REAL_CC
			compile=1
			opt=1
			shift
			;;
		--dump-config )   
			shift
			config=$1
			dumpconfig $config 
			exit $?
			;;
		--force )
			shift
			opt_force=1
			opt=1
			;;
		--no-mbuf )
			opt_nombuf="--no-mbuf"
			shift
			opt=1
			;;
		--compat )
			$CFLAGS="$CFLAGS -I$MLIBDIR/compat"
			echo Warning: compat headers >&2
			shift
			opt=1
			;;
		--exec ) #compile and run 
			echo Sorry, not working atm.
			echo "(TODO) - repair option --exec"
			exit 1
			shift
			opt=1
			opt_exec=1
			opt_execname=`basename $1 .c`
			opt_compilename=`echo $1 | sed 's/.*\///'`
			REAL_CC="$REAL_CC -o $opt_execname" # -x c <(cat $1)"
			BINARY=$opt_execname
			verbose compiling $1 to $opt_execname: $REAL_CC
			#shift
			;;
		--autoconf | -ac )
			shift
			opt_autoconf=1
			opt=1
			#echo "Invoking: $0 --config <($0 --dump-conf $@) $@ --include <($0 --dump-minilib)" 
			genconfig $@ 
			echo Ok
			source <( genconfig $@ ) > minilib.mconf.h
			echo '#include "minilib.h"' >> minilib.mconf.h
			if [ "$opt_ldscript" != "" ]; then
				$0 --ldscript $opt_ldscript $@ -include minilib.mconf.h -include <($0 --dump-minilib) -DINCLUDESRC
			else
				$0 $@ -include minilib.mconf.h -include <($0 --dump-minilib) -DINCLUDESRC
			fi
			exit $?
			;;
		--dump-conf )
			shift
			defaultvalues
			genconfig $@
			exit $?
			;;
		--dump-sources )
			shift
			dumpsources $@
			exit $?
			;;
		--genconf )
			shift
			outfile=$1
			if [ -e $outfile ]
			then
				if [ $opt_force == 1 ]
				then
					echo "Overwriting $outfile"
				else
					echo "$outfile already exists. Won't overwrite without --force."
					exit 1
				fi
			fi
			echo generating config in $outfile
			defaultvalues
			shift
			genconfig $@ > $outfile
			exit $?
			;;
		--ldscript )
			shift
			opt_ldscript=$1
			if [ -e "$MLIBDIR/ldscripts/ld.script.$1" ]; then
				LDSCRIPTFILE=",-T$MLIBDIR/ldscripts/ld.script.$1"
			else
				LDSCRIPT="$1"
			fi
			shift
			opt=1
			;;
		--ldscript-file )
			shift
			LDSCRIPT=",-T$1"
			shift
			opt=1
			;;
		--shrinkelf )
			shift
			opt=1
			opt_shrinkelf=1
			;;
		-h | --help )   
			usage
			exit
			;;
		--license )
			echo "$LICENSE"
			echo
			exit
			;;
		--fulldebug )
			shift
			opt=1
			fulldebug=1
			;;

	esac
done

#verbose Parsing args done.

if [ "$1" = "" ]; then
	if [ "$compile" != "1" ]; then
		# Assume, everything is alright and there havent been parameters forgotten
		# $1 will have the arguments for gcc, so continue below..
		exit 0;
	fi
fi

defaultvalues

exec_gcc $@

ret=$?

if [ $ret != 0 ]; then
	echo $RED Error.
	#echo Failed command:
	#echo $REAL_CC $CFLAGS$LDSCRIPT $@
	command errno > /dev/null
	r=$?
	if [ $r ]; then
		echo errno: $ret
	else
		errno $ret
	fi
	echo $NORM
	exit $ret
else
	echo $LGREEN"OK"$NORM
fi

if [ "$opt_shrinkelf" = "1" ]; then
	if [ -e $MLIBDIR/tools/shrinkelf ]; then
		echo_verbose Executing shrinkelf
		stat -c "size of $outputfile: %sB" $outputfile
		$MLIBDIR/tools/shrinkelf $outputfile
		stat -c "    shrinked to: %sB" $outputfile
		exit $?
	fi
fi

if [ "$opt_exec" = "1" ]; then
	verbose execute: $opt_execname
	verbose
	./$opt_execname
	ret=$?
fi

verbose -e ""
exit $ret

fi
# end of if PARSE==1
# the loop for sourcing the whole script.
PARSE=0


# ansi colors
BLACK=""$'\033[30m'""
GRAY=""$'\033[01;30m'""
LGREEN=""$'\033[01;32m'""
GREEN=""$'\033[32m'""
LRED=""$'\033[01;31m'""
RED=""$'\033[31m'""
YELLOW=""$'\033[01;33m'""
BROWN=""$'\033[33m'""
LBLUE=""$'\033[01;34m'""
BLUE=""$'\033[34m'""
BOLD=""$'\033[01;39m'""
NORM=""$'\033[00m'""
PINK=""$'\033[01;35m'""
MAGENTA=""$'\033[00;35m'""
LMAGENTA=""$'\033[01;35m'""
CYAN=""$'\033[36m'""
LCYAN=""$'\033[01;36m'""
WHITE=""$'\033[01;37m'""
LGRAY=""$'\033[37m'""


# compile gcc options and execute gcc
exec_gcc(){
	opt_args="$@"


	if [ -n "$DUMMYHEADER" ]; then
		if [ -d "$MLIBDIR/dummyheader" ]; then
			opt_args="-I$MLIBDIR/dummyheader $opt_args"
		else
			if [ ! -d $BUILDDIR ]; then
				verbose Create builddir: $BUILDDIR
				mkdir $BUILDDIR
			fi
			createdummyheaders
			opt_args="-I$BUILDDIR/dummyheader $opt_args"
		fi

	fi


	if [ -z "$sources" ]; then
		export sources=`echo $opt_args | sed -E 's/.*\s(\S\S*\.c).*/\1/'`
		opt_args=`echo $opt_args | sed -E 's/(.*\s)(\S\S*\.c)(.*)/\1 \3/'`
		if [ -z "$sources" ]; then
			export sources=$config
		fi
	fi
	SRC="$0 --dump-sources"

	if [ "$opt_execname" != "" ]; then # compile and execute
		opt_args="-x c <(sed '1s/^#!/\/\/ #!/' $opt_compilename) 2>&1 | sed 's/^.*fd.[0-9]*/'$opt_compilename'/'"
	fi

	if [ -n "$SHOWSOURCES" ]; then
		opt_args="-E -P $opt_args"
	else

		if [ -n "$outputfile" ]; then
			echo $opt_args | sed '/-o /Q1' > /dev/null
			if [ $? = 0 ]; then
				opt_args="-o $outputfile $opt_args"
			fi
			echo compile: $outputfile
		fi
	fi

	if [ -n "$PICFLAG" ]; then
		CFLAGS="$PICFLAG $CFLAGS"
	fi

	if [ -n "$NOCOLOR" ]; then
		colorize=""
	fi

	INCLUDEMINILIB=""
	if [ "$useincludedminilib" == "1" ]; then
		INCLUDEMINILIB="$0 --dump-minilib"
		#INCLUDEMINILIB="-include <($0 --dump-minilib)"
	else
		INCLUDEMINILIB="echo '#include <minilib.h>'"
		opt_args=" -I$MLIBDIR $opt_args"
	fi


	if [ "$useincludedminilib" == "0" ]; then
		if [ -e "$MLIBDIR/ldscripts/ld.script.$LDSCRIPT" ]; then
			LDSCRIPTFILE=",-T$MLIBDIR/ldscripts/ld.script.$LDSCRIPT"
		fi
	fi

	if [ "$LDSCRIPTFILE" == "" ]; then
		LDSCRIPTFILE=",-T<($0 --dump ld_script_$LDSCRIPT)"
	fi
	if [ "$fulldebug" == 1 ]; then
		LDFLAGS=""
		#LDSCRIPTFILE=",-T<(ld --verbose | sed -e '1,/^===/d;/^===/,\$d')"
		LDSCRIPTFILE=""
		dbgfile=`echo -n $opt_args | sed -E 's/.*-o (\S*).*/\1.dbg/'`
		opt_args=`echo -n $opt_args | sed -E 's/-o \S*//'`
		OPTFLAG='-g'
	fi

	INCLUDECONFIG=""
	if [ "$opt_config" == "" ]; then
		opt_config=$sources
	fi
		if [ "$useincludedminilib" == "0" ]; then
			INCLUDECONFIG="$0 --dump-config $opt_config"
		else 
			#	INCLUDECONFIG="-include <($0 --useincludedminilib --dump-config $opt_config;$0 --useincludedminilib --dump-minilib)"
			#($0 --useincludedminilib --dump-config $opt_config;$0 --dump-minilib) > minilib.tmp.h
			#($0 --useincludedminilib --dump-config $opt_config;$0 --useincludedminilib --dump-minilib) > minilib.tmp.h
			INCLUDECONFIG="$0 --useincludedminilib --dump-config $opt_config" #) > minilibcfg.tmp.h
			#$0 --useincludedminilib --dump-minilib > minilib.tmp.h
			#INCLUDECONFIG="-include minilibcfg.tmp.h -include minilib.tmp.h"
		fi


	if [ "$useincludedminilib" == "0" ]; then
		CFLAGS="$INCLUDEDIRS $CFLAGS"
	fi


	echo_verbose "$REAL_CC $CFLAGS $LDFLAGS$LDSCRIPTFILE $OPTFLAG $STRIPFLAG $opt_args"


	if [ "$debuglog" = "1" ]; then
		let i=1;
		while [ -e "debuglog.$i" ] 
		do 
			let i++
		done
		eval "($INCLUDECONFIG;$INCLUDEMINILIB;$SRC ) | $REAL_CC $CFLAGS $LDFLAGS$LDSCRIPTFILE $OPTFLAG $STRIPFLAG $opt_args -E -x c - > debuglog.$i"
	fi


	if [ "$fulldebug" == 1 ]; then
		echo -e "\n   $YELLOW switching to fulldebug mode.$NORM\n"

		echo $CYAN eval "$REAL_CC $CFLAGS $opt_args -E" $NORM 
		echo

		eval "($INCLUDECONFIG;$INCLUDEMINILIB;$SRC ) | $REAL_CC $CFLAGS $opt_args -E -x c - | sed '/^#/d' > $dbgfile.c"

		echo $CYAN "eval $REAL_CC $CFLAGS -O0 -g -o $dbgfile $dbgfile.c"$NORM

		eval $REAL_CC $CFLAGS -O0 -g -o $dbgfile $dbgfile.c $colorize
	else
		# execute gcc
		if [ "$dump_cmd" = "1" ]; then
			echo $REAL_CC $INCLUDECONFIG $INCLUDEMINILIB $CFLAGS $LDFLAGS$LDSCRIPTFILE $OPTFLAG $STRIPFLAG $opt_args $colorize
		else
			verbose eval "($INCLUDECONFIG;$INCLUDEMINILIB;$SRC ) | $REAL_CC $CFLAGS $LDFLAGS$LDSCRIPTFILE $OPTFLAG $STRIPFLAG $opt_args -x c -"
			eval "($INCLUDECONFIG;$INCLUDEMINILIB;$SRC ) | $REAL_CC $CFLAGS $LDFLAGS$LDSCRIPTFILE $OPTFLAG $STRIPFLAG $opt_args -x c - $colorize"
			#eval $REAL_CC $INCLUDECONFIG $INCLUDEMINILIB $CFLAGS $LDFLAGS$LDSCRIPTFILE $OPTFLAG $STRIPFLAG $opt_args $colorize
		fi
		#$REAL_CC $CFLAGS$LDSCRIPT $@
	fi
	return ${PIPESTATUS[1]}
}


# Parse config.
# Also set variables in the current instance of minimake
checkconfig (){
	#( (echo "#define macro(a1,a2) M a1" && echo "macro(12,13)" && cat $1 ) | gcc -include tmp2.h -E - )
	echo_verbose minilibconfig: parse $1
	#ERR=$((source $1 > $1.h) 2>&1)
	#echo '#include "minilib.h"' >> $1.h
	#sed "s/command/function/"
	ERR= source $1 2>&1 > /dev/null
	if [ -n "$ERR" ]; 
	then
		echo -e "Error parsing config in $1: \n $ERR" >&2
		exit 1;
	fi
	# set unset values to defaults
	defaultvalues
	# dump parts of the config, when verbose
	echo_verbose minilibdir: $MLIBDIR
	echo_verbose ARCH: $MINI_ARCH
	echo_verbose OS: $MINI_OS
	echo_verbose ldscript: $LDSCRIPT
}


dumpconfig (){

	exec 3>&1 4>&2 #open fd 3
	E=$( { source $1 1>&3;} 2>&1 ) 3>&1  # redirect error messages to fd3

	if [ "$E" != "" ]; 
	then
		echo -e "Error parsing config in $1: \n\033[1;31m $E \033[1;37m" >&2
		#exit 1;
	fi

	if [ "$useincludedminilib" == "0" ]; then
		echo '#include "minilib.h"'
		#else
		#		echo -e "#define mini_INCLUDESRC\n"

	fi
}

dumpsources() {
	#opt_args="$@ $sources"
	defaultvalues
	echo $sources | sed -E 's/(\S\S*)\s*/#include "\1"\n/g'
	verbose Source: $@
}

genconfig(){
cat << 'ENDL'
#mlconfig
ifdef undef
# ============================================================================
# either type 'make -f minilib.conf'
# to use this combined config and makefile generated by minimake,
# compile and download when needed (ca.100kB) with minilib.
# (https://github.com/michael105/minilib)
# 
#
# Alternatively, when the script minimake with the embedded minilib 
# is already present, 'minimake --config "thisfile"' 
# compiles without fetching minilib again.
# ============================================================================
#
# 
# Minilib and the accompanying tools are licensed under a 
# BSD-style opensource license with attribution.
# I did my best and I'm using minilib myself,
# but cannot give any guarantees for any functionality
# of an opensource library or the accompanying tools.
#
# (Disclaimer)
# THIS SOFTWARE IS PROVIDED BY THE COPYRIGHT HOLDERS AND CONTRIBUTORS 'AS IS' AND
# ANY EXPRESS OR IMPLIED WARRANTIES, INCLUDING, BUT NOT LIMITED TO, THE IMPLIED
# WARRANTIES OF MERCHANTABILITY AND FITNESS FOR A PARTICULAR PURPOSE ARE
# DISCLAIMED. IN NO EVENT SHALL Michael Myer BE LIABLE FOR ANY
# DIRECT, INDIRECT, INCIDENTAL, SPECIAL, EXEMPLARY, OR CONSEQUENTIAL DAMAGES
# (INCLUDING, BUT NOT LIMITED TO, PROCUREMENT OF SUBSTITUTE GOODS OR SERVICES;
# LOSS OF USE, DATA, OR PROFITS; OR BUSINESS INTERRUPTION) HOWEVER CAUSED AND
# ON ANY THEORY OF LIABILITY, WHETHER IN CONTRACT, STRICT LIABILITY, OR TORT
# (INCLUDING NEGLIGENCE OR OTHERWISE) ARISING IN ANY WAY OUT OF THE USE OF THIS
# SOFTWARE, EVEN IF ADVISED OF THE POSSIBILITY OF SUCH DAMAGE.
#
# The full license is available at the webpage of minilib.
# https://github.com/michael105/minilib
# Michael (misc) Myer, 2021
#


ENDL


if [ "$opt_nombuf" = "" ]; then
		echo '# Save default. The minibuf is needed for malloc, printf, and file streams.
mini_buf 4000'
	else 
	 	echo  '# Disabled mbuf. The minibuf is needed for malloc, printf, and file streams.
# mini_buf 1024'
fi

cat << 'ENDL'						
# define headerguards, to prevent parsing the standard header
HEADERGUARDS

# Startup function
mini_start

# put the globals on stack. 
# (past the last used return address, but before the environment.
# there's more documentation in the readme/reference)
# reserves a fixed register as pointer to the globals
# globals_on_stack

# define var (#define var arg)
# DEFINE var arg

# optimization Flag. Os,O1,O2 might be save. O3 is known to cause sometimes trouble
OPTFLAG -Os

# (with debug info)
#OPTFLAG '-g -Og'

# stripflag ( defaults to -s)
#STRIPFLAG

# Build minilib source
INCLUDESRC

# the ldscript to use
# Can be one of: default, onlytext, textandbss, onlytext_tiny
LDSCRIPT default

# Shrink the compiled binary with shrinkelf
#SHRINKELF

# generate debug info (-Og -g). Overwrites OPTFLAG, STRIPFLAG and SHRINKELF
#DEBUG

# generate debug info, including the source of minilib itself. (-Og -g). 
# Overwrites OPTFLAG, STRIPFLAG and SHRINKELF
#FULLDEBUG


# list of source files, supplied to gcc
# $1 expands to the current filename 
ENDL

echo SOURCES $@
#SOURCES $1 file1.c file2.c

echo -e '\n# the binary to be compiled ( -o $BINARY )'
echo BINARY `basename $1 .c`

echo -e '\n# function switches. Only functions named below will be compiled\n'

if [ "$debuglog" = "1" ]; then
		let i=1;

		while [ -e "debuglog.$i" ] 
		do 
				let i++
		done
		(echo "$headerguards_h" "$genconfigmacros_h"; cat $@) \
			| $REAL_CC $CFLAGS -Dincluded_minilib_h -E -x c - > debuglog.$i
		echo $REAL_CC $CFLAGS $@ >> debuglog.$i
		let i++
		echo "$headerguards_h" "$genconfigmacros_h" >> debuglog.$i
fi

(echo "$headerguards_h" "$genconfigmacros_h"; cat $@) \
	| $REAL_CC $CFLAGS -Dincluded_minilib_h -E -x c - \
	| sed 's/_M_MLIB_/\n_M_MLIB_/g' | sed -n 's/^.*_M_MLIB_\(.*\)_M_.*/mini_\1/p' \
	| sort -u 2>/dev/null 


cat << 'ENDL'


return
endif
# configuration part ends, 
# embedded makefile starts below


VERSION := "20211012"
urlbase := "https://raw.githubusercontent.com/michael105/minilib/download"

fetch = $(shell ((curl $(urlbase)/$(VERSION)/$(1).gz > $(1).gz) || (wget $(urlbase)/$(VERSION)/$(1).gz)) && gunzip $(1).gz)


default: Makefile.minilib minilib.h check compile


compile:
	$(info Compile)
	make -f Makefile.minilib CONF="$(lastword $(MAKEFILE_LIST))"


Makefile.minilib:
	$(info "Download Makefile.minilib (Version $(VERSION))from github")
	$(call fetch,Makefile.minilib)


minilib.h:
	$(info "Download minilib.h (Version $(VERSION))from github")
	$(call fetch,minilib.h)


.ONESHELL:
check:
	@echo "Checking sha256 sums"
	echo -e "$(SHA256SUMS)" | sha256sum -c || exit
	echo ok 


# checksums 
define SHA256SUMS =
a188c44dde75695da40015d61ec5250723f21ab7484062c7f4879c0b872fc9f4  Makefile.minilib
endef


ENDL
	
return $?

}



createdummyheaders(){
	if [ ! -d $BUILDDIR ]; then
		mkdir $BUILDDIR
	fi
	if [ ! -d "$BUILDDIR/dummyheader" ]; then
		mkdir $BUILDDIR/dummyheader
	fi

	tmp=`pwd`
	cd $BUILDDIR/dummyheader && \
	mkdir bits sys net arpa scsi netinet protocols && \
	for i in bits/uClibc_config.h bits/posix_opt.h bits/local_lim.h bits/fcntl.h bits/byteswap.h bits/shm.h bits/epoll.h bits/sem.h bits/mman.h bits/sigcontext.h bits/msq.h bits/environments.h bits/fenv.h bits/wordsize.h bits/endian.h bits/mathdef.h bits/stat.h bits/mathinline.h bits/setjmp.h bits/byteswap-16.h bits/wchar.h bits/uClibc_touplow.h bits/uClibc_charclass.h bits/getopt_int.h bits/socket_type.h bits/utmp.h bits/errno.h bits/statx.h bits/huge_vall.h bits/termios.h bits/ioctls.h bits/sigaction.h bits/typesizes.h bits/sigthread.h bits/fenvinline.h bits/uClibc_local_lim.h bits/uClibc_locale.h bits/xopen_lim.h bits/confname.h bits/uClibc_page.h bits/timerfd.h bits/dirent.h bits/byteswap-common.h bits/huge_valf.h bits/stdio.h bits/waitstatus.h bits/utsname.h bits/eventfd.h bits/ipc.h bits/posix1_lim.h bits/uClibc_clk_tck.h bits/mman-shared.h bits/siginfo.h bits/netdb.h bits/stdio_lim.h bits/nan.h bits/waitflags.h bits/mqueue.h bits/statvfs.h bits/in.h bits/uClibc_alloc.h bits/resource.h bits/mathcalls.h bits/uio.h bits/sigset.h bits/kernel-features.h bits/poll.h bits/mman-common.h bits/signum.h bits/huge_val.h bits/locale.h bits/utmpx.h bits/signalfd.h bits/sigstack.h bits/select.h bits/statfs.h bits/ioctl-types.h bits/inf.h bits/uClibc_posix_opt.h bits/dlfcn.h bits/uClibc_stdio.h bits/sockaddr.h bits/sched.h bits/socket.h bits/elfclass.h bits/initspin.h bits/posix2_lim.h bits/types.h bits/time.h bits/getopt.h bits/inotify.h bits/cmathcalls.h bits/mman-linux.h bits/sysnum.h bits/semaphore.h bits/pthreadtypes.h sys/perm.h sys/ucontext.h sys/io.h sys/reg.h sys/debugreg.h sys/procfs.h sys/user.h sys/timerfd.h sys/ptrace.h sys/prctl.h sys/fanotify.h sys/epoll.h sys/acct.h sys/random.h sys/eventfd.h sys/signalfd.h sys/inotify.h sys/vfs.h sys/swap.h sys/timeb.h sys/types.h sys/timex.h sys/syscall.h sys/socket.h sys/select.h sys/ipc.h sys/kdaemon.h sys/kd.h sys/unistd.h sys/dir.h sys/sysmacros.h sys/reboot.h sys/msg.h sys/mman.h sys/statvfs.h sys/bitypes.h sys/time.h sys/xattr.h sys/syslog.h sys/file.h sys/socketvar.h sys/times.h sys/poll.h sys/mount.h sys/stat.h sys/shm.h sys/un.h sys/statfs.h sys/personality.h sys/resource.h sys/fcntl.h sys/wait.h sys/utsname.h sys/fsuid.h sys/signal.h sys/errno.h sys/quota.h sys/param.h sys/sem.h sys/soundcard.h sys/cdefs.h sys/sendfile.h sys/ioctl.h sys/ttydefaults.h sys/sysinfo.h sys/queue.h sys/termios.h sys/vt.h sys/mtio.h sys/klog.h sys/uio.h sys/sysctl.h net/if_arp.h net/if.h net/if_slip.h net/ppp_defs.h net/route.h net/if_shaper.h net/ethernet.h net/ppp-comp.h net/if_packet.h net/if_ppp.h arpa/ftp.h arpa/nameser.h arpa/tftp.h arpa/inet.h arpa/nameser_compat.h arpa/telnet.h scsi/scsi_ioctl.h scsi/sg.h scsi/scsi.h netinet/tcp.h netinet/ether.h netinet/in_systm.h netinet/igmp.h netinet/if_ether.h netinet/in.h netinet/udp.h netinet/ip6.h netinet/if_tr.h netinet/ip.h netinet/icmp6.h netinet/if_fddi.h netinet/ip_icmp.h fpu_control.h thread_db.h pthread.h semaphore.h libgen.h ucontext.h stdint.h grp.h limits.h fts.h termio.h syscall.h mqueue.h utmpx.h inttypes.h pwd.h regex.h langinfo.h unistd.h a.out.h tar.h wchar.h strings.h printf.h search.h time.h fnmatch.h sysexits.h glob.h elf.h utmp.h stdlib.h math.h wordexp.h link.h syslog.h utime.h ttyent.h poll.h malloc.h byteswap.h nl_types.h features.h complex.h error.h uchar.h stdio_ext.h lastlog.h fcntl.h paths.h wait.h values.h string.h sched.h ifaddrs.h dlfcn.h endian.h signal.h errno.h gnu-versions.h memory.h mntent.h resolv.h crypt.h cpio.h ar.h setjmp.h regexp.h netdb.h locale.h shadow.h stdio.h tgmath.h pty.h ulimit.h ieee754.h getopt.h spawn.h termios.h sgtty.h err.h ftw.h fenv.h wctype.h dirent.h alloca.h execinfo.h ctype.h assert.h protocols/routed.h protocols/talkd.h protocols/timed.h protocols/rwhod.h
	do 
		touch $i
	done
	cd $tmp
}



# ==================================================
# configfiles and embedded scripts start below. 
# They are generated and included by "make minimake"
# All changes below will be overwritten.
# ==================================================

# prefer the genconfig script, if present.
if [ -e $MLIBDIR/scripts/genconfig.sh ]; then
	#echo_verbose Parsing genconfig.sh
	source $MLIBDIR/scripts/genconfig.sh
else
	verbose "Parsing builtin config"	

# --- genconfig-start: TMPL ---
#
# Script, updated from "make header" / genheaders.pl
# The functions are callen from minilib.conf 
# (or another config file)
# The output of this script generates minilib.conf.h
# 

# empty, to enable parsing 'makefiles'
ifdef(){ 
	echo -n
}

globalregister(){
	if [ ! -z $1 ]; then 
		echo "#define mini_globalregister $1" 
	else 
		echo  '#define mini_globalregister "r15"'
	fi
}


mini_errno(){
	echo "#define mini_errno $1"
}

compile(){
	for f in `echo $* | tr ',' ' '`;
	do "mini_$f"
	done
}

COMPILE(){
	for f in `echo $* | tr ',' ' '`;
	do "mini_$f"  
	done
}

VERBOSE(){
	export VERBOSE="1"
}

mini_start(){
	echo "#define mini_start $1"
}
mini_exit(){
	echo "#define mini_exit $1"
}

mini_setjmp(){
	echo "#define mini_setjmp"
}

mini_longjmp(){
	mini_setjmp
}

LDSCRIPT(){
	export LDSCRIPT="$1" 
}

PICFLAG(){
	export PICFLAG="$1" 
}


shrinkelf(){
	export opt_shrinkelf=1
}

SHRINKELF(){
	export opt_shrinkelf=1
}

ARCH(){
	echo "#define MINILIB_ARCH $1"
	export MINI_ARCH=$1
}
OS(){
	echo "#define MINILIB_OS $1"
	export MINI_OS=$1
}
mini_buf(){
	echo "#define mini_buf $1"
}

mini_INCLUDESRC(){
	echo "#define INCLUDESRC"
}

OPTIMIZE(){
	echo "#define mini_OPTIMIZE"
}

BINARY(){
	export outputfile=$1
	export BINARY=$1
}

SOURCES(){
	export sources="$*"
	export SOURCES="$*"
}

DUMMYHEADER(){
	export DUMMYHEADER="1"
}

INCLUDESRC(){
	echo "#define INCLUDESRC"
}

mini_GETOPTS(){
	echo "#define mini_GETOPTS"
}

# The (optimization)-flag, gcc is callen with.
# Either leave it blank, or set it to e.g. '-Os'
# -O2 and higher optimizations seem to give trouble.
# -Os, -O0, -O1 passed all tests here and might be save.
OPTFLAG(){
	if [ -z "$OPTFLAG" ]; then
		export OPTFLAG=$1
	fi
}

# The stripflag (defaults to -s)
STRIPFLAG(){
	if [ -z "$STRIPFLAG" ]; then
		export STRIPFLAG=$1
	fi
}


# Generate debug symbols - don't optimize, don't strip
DEBUG(){
	export OPTFLAG='-Og -g'
	export STRIPFLAG=""
	export opt_shrinkelf=0
}


# Generate debug symbols, including minilib's source.
FULLDEBUG(){
	export opt_shrinkelf=0
	export fulldebug=1
}


# Generate debug symbols, including minilib itself
# A new outputfile ( sourcefilename.dbg.c ) containing the 
# used parts of the minilib source is generated
# and compiled to make this work

mini_HEADERGUARDS(){
	echo "#define HEADERGUARDS"
}


HEADERGUARDS(){
	echo "#define HEADERGUARDS"
}


mini_syscalls(){
	echo "#define mini_syscalls"
}

globals_on_stack(){
	echo "#define mini_globals_on_stack"
}

mini_globals(){
	echo "#define mini_globals"
}

define(){
	param=$1
	shift
	echo "#define" $param \"$*\"
}

DEFINE(){
	param=$1
	shift
	echo "#define" $param \"$*\"
	#echo "#define" $1 $2
}

embed(){
	export embed="$*"
}

mini_getopt(){
	echo "#define mini_getopt"
}

mini_getopt_long(){
	echo "#define mini_getopt_long"
}


mini_define(){
	if [ ! -z $2 ]; then 
		echo "#define mini_$1 $2" 
	else 
		echo  "#define mini_$1 $1"
	fi
}


