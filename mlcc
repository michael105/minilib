		.text           :
		{
				
		 				*(__plugin_export*)

				/* *(.dynbss)
				 *(*mlgl)  */
				/* *(.rodata .rodata.* .gnu.linkonce.r.*) */
				*(.text.unlikely .text.*_unlikely)
						*(.text._start)
						*(.text.exit .text.exit.*)
						*(.text._startup)
						*(.text.main)
						*(.text.hot .text.hot.*)
						*(.text .stub .text.* .gnu.linkonce.t.*)
