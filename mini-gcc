  echo "#define mini_itodec generate" 
}
mini__symlinkat(){ 
  echo "#define mini__symlinkat generate" 
}
mini_sigismember(){ 
  echo "#define mini_sigismember generate" 
}
mini__quotactl(){ 
  echo "#define mini__quotactl generate" 
