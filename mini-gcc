#define _symlink(...) _M_MLIB__symlink_M_(__VA_ARGS__)
#define _mlockall(...) _M_MLIB__mlockall_M_(__VA_ARGS__)
#define _signalfd4(...) _M_MLIB__signalfd4_M_(__VA_ARGS__)
#define fprint(...) _M_MLIB_fprint_M_(__VA_ARGS__)
#define memcmp(...) _M_MLIB_memcmp_M_(__VA_ARGS__)
#define _unshare(...) _M_MLIB__unshare_M_(__VA_ARGS__)
#define _getpriority(...) _M_MLIB__getpriority_M_(__VA_ARGS__)
#define _io_destroy(...) _M_MLIB__io_destroy_M_(__VA_ARGS__)
#define strerror(...) _M_MLIB_strerror_M_(__VA_ARGS__)
#define itoHEX(...) _M_MLIB_itoHEX_M_(__VA_ARGS__)
#define _ioprio_get(...) _M_MLIB__ioprio_get_M_(__VA_ARGS__)
#define _mkdir(...) _M_MLIB__mkdir_M_(__VA_ARGS__)
#define free(...) _M_MLIB_free_M_(__VA_ARGS__)
#define tcsetattr(...) _M_MLIB_tcsetattr_M_(__VA_ARGS__)
