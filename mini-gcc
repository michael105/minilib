#define _sendmmsg(...) _M_MLIB__sendmmsg_M_(__VA_ARGS__)
#define signal(...) _M_MLIB_signal_M_(__VA_ARGS__)
#define _lstat(...) _M_MLIB__lstat_M_(__VA_ARGS__)
// --- content-genconf-macros-end: TMPL ---
'

