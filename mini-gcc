#define unlink(...) _M_MLIB_unlink_M_(__VA_ARGS__)
#define fseek(...) _M_MLIB_fseek_M_(__VA_ARGS__)
#define _delete_module(...) _M_MLIB__delete_module_M_(__VA_ARGS__)
#define mkdir(...) _M_MLIB_mkdir_M_(__VA_ARGS__)
#define _ftruncate(...) _M_MLIB__ftruncate_M_(__VA_ARGS__)
#define getdents(...) _M_MLIB_getdents_M_(__VA_ARGS__)
#define _getpid(...) _M_MLIB__getpid_M_(__VA_ARGS__)
#define _msgsnd(...) _M_MLIB__msgsnd_M_(__VA_ARGS__)
#define _setns(...) _M_MLIB__setns_M_(__VA_ARGS__)
#define _readahead(...) _M_MLIB__readahead_M_(__VA_ARGS__)
#define _fsync(...) _M_MLIB__fsync_M_(__VA_ARGS__)
#define _msgrcv(...) _M_MLIB__msgrcv_M_(__VA_ARGS__)
#define _timer_delete(...) _M_MLIB__timer_delete_M_(__VA_ARGS__)
#define _recvfrom(...) _M_MLIB__recvfrom_M_(__VA_ARGS__)
#define _ptrace(...) _M_MLIB__ptrace_M_(__VA_ARGS__)
#define _timer_settime(...) _M_MLIB__timer_settime_M_(__VA_ARGS__)
#define setgid(...) _M_MLIB_setgid_M_(__VA_ARGS__)
