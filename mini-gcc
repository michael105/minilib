  echo "#define mini__sethostname generate" 
}
mini__getpid(){ 
  echo "#define mini__getpid generate" 
}
mini_utime(){ 
  echo "#define mini_utime generate" 
}
mini__statfs(){ 
  echo "#define mini__statfs generate" 
