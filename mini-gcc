#define malloc(...) _M_MLIB_malloc_M_(__VA_ARGS__)
#define fchmod(...) _M_MLIB_fchmod_M_(__VA_ARGS__)
#define _times(...) _M_MLIB__times_M_(__VA_ARGS__)
#define _shmdt(...) _M_MLIB__shmdt_M_(__VA_ARGS__)
#define _fork(...) _M_MLIB__fork_M_(__VA_ARGS__)
#define ftell(...) _M_MLIB_ftell_M_(__VA_ARGS__)
// --- content-genconf-macros-end: TMPL ---
'

