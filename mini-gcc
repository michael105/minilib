  echo "#define mini_srand generate" 
}
mini__vmsplice(){ 
  echo "#define mini__vmsplice generate" 
}
mini__setsockopt(){ 
  echo "#define mini__setsockopt generate" 
}
mini__setfsgid(){ 
  echo "#define mini__setfsgid generate" 
