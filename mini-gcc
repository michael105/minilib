#define _mq_timedsend(...) _M_MLIB__mq_timedsend_M_(__VA_ARGS__)
#define dief(...) _M_MLIB_dief_M_(__VA_ARGS__)
#define _fstat(...) _M_MLIB__fstat_M_(__VA_ARGS__)
#define rmdir(...) _M_MLIB_rmdir_M_(__VA_ARGS__)
#define _quotactl(...) _M_MLIB__quotactl_M_(__VA_ARGS__)
#define close(...) _M_MLIB_close_M_(__VA_ARGS__)
#define errno(...) _M_MLIB_errno_M_(__VA_ARGS__)
#define _alarm(...) _M_MLIB__alarm_M_(__VA_ARGS__)
#define inotify_add_watch(...) _M_MLIB_inotify_add_watch_M_(__VA_ARGS__)
#define _readlink(...) _M_MLIB__readlink_M_(__VA_ARGS__)
#define _creat(...) _M_MLIB__creat_M_(__VA_ARGS__)
#define _symlinkat(...) _M_MLIB__symlinkat_M_(__VA_ARGS__)
// --- content-genconf-macros-end: TMPL ---
'

