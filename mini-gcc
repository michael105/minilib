  echo "#define mini__pause generate" 
}
mini_sigemptyset(){ 
  echo "#define mini_sigemptyset generate" 
}
mini__tee(){ 
  echo "#define mini__tee generate" 
}
mini_strchr(){ 
  echo "#define mini_strchr generate" 
