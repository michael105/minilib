MAGENTA=""$'\033[00;35m'""
LMAGENTA=""$'\033[01;35m'""
CYAN=""$'\033[36m'""
LCYAN=""$'\033[01;36m'""
WHITE=""$'\033[01;37m'""
LGRAY=""$'\033[37m'""


touchdummyheaders(){
	if [ ! -d $BUILDDIR ]; then
		mkdir $BUILDDIR
	fi
	if [ ! -d "$BUILDDIR/dummyheader" ]; then
		mkdir $BUILDDIR/dummyheader
	fi

	tmp=`pwd`
	cd $BUILDDIR/dummyheader && \
	mkdir bits sys net arpa scsi netinet protocols && \
	for i in bits/uClibc_config.h bits/posix_opt.h bits/local_lim.h bits/fcntl.h bits/byteswap.h bits/shm.h bits/epoll.h bits/sem.h bits/mman.h bits/sigcontext.h bits/msq.h bits/environments.h bits/fenv.h bits/wordsize.h bits/endian.h bits/mathdef.h bits/stat.h bits/mathinline.h bits/setjmp.h bits/byteswap-16.h bits/wchar.h bits/uClibc_touplow.h bits/uClibc_charclass.h bits/getopt_int.h bits/socket_type.h bits/utmp.h bits/errno.h bits/statx.h bits/huge_vall.h bits/termios.h bits/ioctls.h bits/sigaction.h bits/typesizes.h bits/sigthread.h bits/fenvinline.h bits/uClibc_local_lim.h bits/uClibc_locale.h bits/xopen_lim.h bits/confname.h bits/uClibc_page.h bits/timerfd.h bits/dirent.h bits/byteswap-common.h bits/huge_valf.h bits/stdio.h bits/waitstatus.h bits/utsname.h bits/eventfd.h bits/ipc.h bits/posix1_lim.h bits/uClibc_clk_tck.h bits/mman-shared.h bits/siginfo.h bits/netdb.h bits/stdio_lim.h bits/nan.h bits/waitflags.h bits/mqueue.h bits/statvfs.h bits/in.h bits/uClibc_alloc.h bits/resource.h bits/mathcalls.h bits/uio.h bits/sigset.h bits/kernel-features.h bits/poll.h bits/mman-common.h bits/signum.h bits/huge_val.h bits/locale.h bits/utmpx.h bits/signalfd.h bits/sigstack.h bits/select.h bits/statfs.h bits/ioctl-types.h bits/inf.h bits/uClibc_posix_opt.h bits/dlfcn.h bits/uClibc_stdio.h bits/sockaddr.h bits/sched.h bits/socket.h bits/elfclass.h bits/initspin.h bits/posix2_lim.h bits/types.h bits/time.h bits/getopt.h bits/inotify.h bits/cmathcalls.h bits/mman-linux.h bits/sysnum.h bits/semaphore.h bits/pthreadtypes.h sys/perm.h sys/ucontext.h sys/io.h sys/reg.h sys/debugreg.h sys/procfs.h sys/user.h sys/timerfd.h sys/ptrace.h sys/prctl.h sys/fanotify.h sys/epoll.h sys/acct.h sys/random.h sys/eventfd.h sys/signalfd.h sys/inotify.h sys/vfs.h sys/swap.h sys/timeb.h sys/types.h sys/timex.h sys/syscall.h sys/socket.h sys/select.h sys/ipc.h sys/kdaemon.h sys/kd.h sys/unistd.h sys/dir.h sys/sysmacros.h sys/reboot.h sys/msg.h sys/mman.h sys/statvfs.h sys/bitypes.h sys/time.h sys